{
  "Routes": [
    {
      //Get all user's of a board
      "DownstreamPathTemplate": "/swagger",
      "DownstreamScheme": "http",
      "ServiceName": "boarddb",
      "UpstreamPathTemplate": "/boarddb/swagger",
      "UpstreamHttpMethod": [ "Get" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://api.localhost",
    "ServiceDiscoveryProvider": {
      "Namespace": "sheper",
      "Type": "pollkube",
      "PollingInterval": 100
    }
  }
}