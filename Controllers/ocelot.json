{
  "Routes": [
    {
      //Get all user's of a board
      "DownstreamPathTemplate": "/board/{boardId}/user",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/board/{boardId}/user",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      //Get specific user of board
      "DownstreamPathTemplate": "/board/{boardId}/user/{userId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/board/{boardId}/user/{userId}",
      "UpstreamHttpMethod": [ "Get" ]
    },
    {
      //Delete user from board
      "DownstreamPathTemplate": "/board/{boardId}/user/{userId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/board/{boardId}/user/{userId}",
      "UpstreamHttpMethod": [ "Delete" ]
    },
    {
      //Update role user from board
      "DownstreamPathTemplate": "/board/{boardId}/user/{userId}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/board/{boardId}/user/{userId}",
      "UpstreamHttpMethod": [ "PUT" ]
    },
    {
      //Create role for user for board
      "DownstreamPathTemplate": "/board/{boardId}/user",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "jsonplaceholder.typicode.com",
          "Port": 443
        }
      ],
      "UpstreamPathTemplate": "/board/{boardId}/user",
      "UpstreamHttpMethod": [ "POST" ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://api.mybusiness.com"
  }
}